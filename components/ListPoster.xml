<?xml version="1.0" encoding="utf-8"?>
<component name="ListPoster" extends="Group">
  <children>
    <Rectangle id="backdrop" />
    <ScrollingText id="Series" horizAlign="center" font="font:SmallestSystemFont" repeatCount="0" visible="false" />
    <Poster id="poster" translation="[2,0]" loadDisplayMode="scaleToFit">
      <PlayedCheckmark id="playedIndicator" translation="[135, 0]" />
    </Poster>
    <ScrollingText id="title" horizAlign="center" font="font:SmallestSystemFont" repeatCount="0" visible="false" />
    <Text id="staticTitle" horizAlign="center" font="font:SmallestSystemFont" wrap="false" />
  </children>
  <interface>
    <field id="itemContent" type="node" onChange="itemContentChanged" />
    <field id="itemWidth" type="integer" />
    <field id="itemHasFocus" type="boolean" onChange="focusChanged" />
  </interface>
</component>